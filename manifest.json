{
	"name": "Bookmarks Manager - Remake",
	"version": "1.0",
	"description": "A remake of Googles bookmarks manager",
	"manifest_version": 2,

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"icons": {
		"128": "icons/collected128.png",
		"16": "icons/collected16.png",
		"48": "icons/collected48.png"
	},
	"chrome_url_overrides": {
		"bookmarks": "bookmarks.html"
	},
	"commands": {
		"_execute_page_action": {
			"description": "Stars popup",
			"suggested_key": {
				"default": "Ctrl+D"
			}
		}
	},
	"chrome_ui_overrides": {
		"bookmarks_ui": {
			"remove_bookmark_shortcut": true,
			"remove_button": true
		}
	},
	"page_action": {
		"default_icon": {
			"19": "icons/collected19.png"
		},
		"default_title": "Star Page"
	},
	"permissions": [ "activeTab", "bookmarks", "bookmarkManagerPrivate", "chrome://favicon/", "identity", "identity.email", "management", "metricsPrivate", "notifications", "preferencesPrivate", "storage", "tabs", "*://*.google.com/*", {"fileSystem": [ "write" ]} ],
